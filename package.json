{
  "name": "enterprisejsapps",
  "version": "0.1.0",
  "description": "Learning enterprise javascript",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "precommit": "yarn run lint",
    "serve": "yarn run build && dotenv node dist/index.js",
    "test:e2e": "./scripts/e2e.test.sh",
    "watch": "nodemon -w src --exec yarn run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eaparks/enterpriseJSApps.git"
  },
  "author": "Ed Parks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eaparks/enterpriseJSApps/issues"
  },
  "homepage": "https://github.com/eaparks/enterpriseJSApps#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "cucumber": "^3.2.1",
    "dotenv-cli": "^1.4.0",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "superagent": "^3.8.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "elasticsearch": "^14.0.0",
    "express": "^4.16.2"
  }
}
